#include <iostream>
using namespace std;

double pembagian ( double a, double b){
	cout << (a / b);
}

int main (){
	cout<< "==HALLO==" << endl;
	cout<< "Ini adalah prototype function pembagian" << endl;
	cout<< endl;
	
	double a, b, hasil;
	cout<< "Masukan nilai yang ingin di bagi"<< endl;
	cin>>a;
	cout<< "Masukan bilangan pembagi"<< endl;
	cin>>b;
	cout<<"Hasil pembagian"<< endl;
	
	hasil = pembagian ( a, b);
}
